	        	<% @a = @a+1 %>
		      	<a class="btn btn-primary btn-block" role="button" data-toggle="collapse" href="#collapse<%=@a%>" aria-expanded="false" aria-controls="collapse<%=@a%>">
					<%= smartphone.modele %>
				</a>
				<div class="collapse" id="collapse<%=@a%>">
					<div class="well">
				    	<% Stock.where(id_smartphone: smartphone.id).to_a.each do |stock| %>
					    	<div>
					    		<%= link_to Panne.find(stock.id_panne).name, {:action => "devis", :stock => stock} %>
			      			</div>
